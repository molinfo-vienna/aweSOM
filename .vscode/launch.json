{
    "version": "0.2.0",
    "debugOptions": ["RedirectOutput", "PyDebug", "ShowReturnValue", "WaitOnAbnormalExit", "WaitOnNormalExit", "Timeout=300"],
    "redirectOutput": true,
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debug Preprocess",
            "type": "python",
            "request": "launch",
            "program": "scripts/preprocess.py",
            "args": [
                "-f", "data.sdf", 
                "-d", "data/MetaTrans", 
                "-s", "10", 
                "-fc", "FC3", 
                "-v", "INFO"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debug Train",
            "type": "python",
            "request": "launch",
            "program": "scripts/train.py",
            "args": [
                "-d", "data/MetaTrans/preprocessed/train", 
                "-lf", "BCE",
                "-hd", "256", 
                "-do", "0.1", 
                "-e", "1000", 
                "-lr", "0.01", 
                "-wd", "0.001", 
                "-bs", "64", 
                "-p", "20", 
                "-dt", "0", 
                "-o", "output/MetaTrans/exp/train", 
                "-hps", "False", 
                "-v", "DEBUG"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debug FineTune",
            "type": "python",
            "request": "launch",
            "program": "scripts/finetune.py",
            "args": [
                "-d", "data/metaQSAR/preprocessed/train", 
                "-lf", "BCE", 
                "-m", "output/MetaTrans/bce/train", 
                "-e", "1000", 
                "-lr", "0.0001", 
                "-wd", "0.001", 
                "-bs", "32", 
                "-p", "20", 
                "-dt", "0", 
                "-o", "output/metaQSAR/bce/finetune", 
                "-hps", "True", 
                "-v", "DEBUG"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debug Test",
            "type": "python",
            "request": "launch",
            "program": "scripts/test.py",
            "args": [
                "-d", "data/metaQSAR/preprocessed/test", 
                "-lf", "BCE", 
                "-m", "output/metaQSAR/bce/train", 
                "-n", "5", 
                "-o", "output/metaQSAR/bce/test/5", 
                "-v", "INFO"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debug Predict",
            "type": "python",
            "request": "launch",
            "program": "scripts/predict.py",
            "args": [
                "-d", "data/metaQSAR/preprocessed/test", 
                "-lf", "BCE", 
                "-m", "output/MetaTrans/bce/train", 
                "-n", "5", 
                "-o", "output/metaQSAR/bce/predict_from_MetaTrans/5", 
                "-v", "INFO"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}