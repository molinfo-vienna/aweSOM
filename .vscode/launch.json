{
    "version": "0.2.0",
    "debugOptions": ["RedirectOutput", "PyDebug", "ShowReturnValue", "WaitOnAbnormalExit", "WaitOnNormalExit", "Timeout=300"],
    "redirectOutput": true,
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debug Preprocess",
            "type": "python",
            "request": "launch",
            "program": "scripts/preprocess.py",
            "args": [
                "-d", "data/nitrosopropylamine", 
                "-f", "data.sdf",
                "-w", "1",
                "-p", "True",
                "-v", "INFO"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debug Train GATv2",
            "type": "python",
            "request": "launch",
            "program": "scripts/train.py",
